{ 
  "appenders": {
  "out":{"type": "console", "category": "console"}, 
  "app":{      
    "type": "dateFile",                 
    "filename": "log/debug.log",    
    "pattern": ".yyyyMMdd", 
    "absolute": false,                 
    "alwaysIncludePattern": true,   
    "keepFileExt":true        
  },
  "profit":{
    "type": "dateFile",                 
    "filename": "log/profit.log",    
    "pattern": ".yyyyMMdd", 
    "absolute": false,                 
    "alwaysIncludePattern": true,  
    "keepFileExt":true           
  } 
},
"categories": {
    "default": { "appenders": [ "out", "app" ], "level": "debug" },
    "profit":{"appenders": [ "profit" ], "level": "debug" }
    }
}
