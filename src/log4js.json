{ 
  "appenders": {
  "out":{"type": "console", "category": "console"}, 
  "app":{      
    "type": "dateFile",                 
    "filename": "log/debug",    
    "pattern": "._yyyyMMdd.log", 
    "absolute": false,                 
    "alwaysIncludePattern": true,    
    "category": "logInfo"              
  },
  "profit":{
    "type": "dateFile",                 
    "filename": "log/profit",    
    "pattern": "._yyyyMMdd.log", 
    "absolute": false,                 
    "alwaysIncludePattern": true,    
    "category": "logInfo"              
  } 
},
"categories": {
    "default": { "appenders": [ "out", "app" ], "level": "debug" },
    "profit":{"appenders": [ "profit" ], "level": "debug" }
    }
}
